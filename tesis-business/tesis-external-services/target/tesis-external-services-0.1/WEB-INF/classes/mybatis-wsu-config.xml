<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="logImpl" value="LOG4J"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
	<typeAliases>
		<typeAlias type="cl.andina.tesis.dto.ClienteDTO" alias="clienteDTO"/>
		<typeAlias type="cl.andina.tesis.dto.MarcaDTO" alias="marcaDTO"/>
	</typeAliases>
    <mappers>
	    <mapper resource="mappings/ClienteServiceMapper.xml"/>
	    <mapper resource="mappings/FormatoProductoServiceMapper.xml"/>
	    <mapper resource="mappings/TipoProductoServiceMapper.xml"/>
	    <mapper resource="mappings/MarcaServiceMapper.xml"/>
    </mappers>

</configuration>